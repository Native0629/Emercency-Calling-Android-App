<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/cajamarca3"
    android:orientation="vertical"
    tools:context=".fragment_noti">

    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/bgoverlay" />
    <!-- TODO: Update blank fragment layout -->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="134dp"
            android:gravity="center"

            android:orientation="vertical">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="113dp"
                android:src="@drawable/starter_logo_full">
            </ImageView>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/linearLa"
            android:layout_width="match_parent"
            android:layout_height="46dp"
            android:layout_gravity="center"
            android:orientation="horizontal"
            android:padding="5dp">

            <TextView

                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"

                android:layout_marginLeft="0dp"
                android:enabled="false"
                android:hint="NOTIFICACIONES"
                android:inputType="textMultiLine"
                android:maxLines="1"
                android:background="@drawable/fragmentreportar_caja"

                android:textColor="#000000"
                android:textColorHint="#fff"
                android:textSize="18dp"
                android:textStyle="bold" />


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="531dp"
            android:orientation="vertical">

            <ListView
                android:id="@+id/lnotifica"
                android:layout_width="match_parent"
                android:layout_height="509dp"
                android:paddingBottom="50dp">

            </ListView>

        </LinearLayout>

    </LinearLayout>


</RelativeLayout>